{
  "expo": {
    "name": "ReallinkApp",
    "slug": "reallinkapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "reallinkapp",
    "splash": {
      "image": "./src/assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#111827"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.reallinkapp.app",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSBluetoothAlwaysUsageDescription": "This app requires Bluetooth to connect with nearby devices.",
        "NSBluetoothPeripheralUsageDescription": "We use Bluetooth to discover and connect to nearby users."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon.png",
        "backgroundColor": "#111827"
      },
      "package": "com.reallinkapp.app",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.BLUETOOTH_SCAN",
        "android.permission.BLUETOOTH_CONNECT",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./src/assets/favicon.png",
      "bundler": "metro",
      "output": "static"
    },
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow ReallinkApp to access your photos.",
          "cameraPermission": "Allow ReallinkApp to access your camera."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow ReallinkApp to use your location."
        }
      ]
    ],
    "extra": {
      "SUPABASE_URL": "https://xwtqbcmdzmxticfzdyhp.supabase.co",
      "SUPABASE_ANON_KEY": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...",
