      "RAZORPAY_KEY_SECRET": "Lg8N19R6b5hTQ7lWubh6yqHb",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d06f6087-00bf-42a9-a6ab-b4e6e0c2e202"
      }
    },
    "owner": "gamer2216"
  }
}
